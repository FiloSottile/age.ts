{
    "name": "age-encryption",
    "version": "0.0.4",
    "main": "dist/index.js",
    "files": [
        "dist/"
    ],
    "repository": "github:FiloSottile/age.ts",
    "type": "module",
    "types": "./dist/index.d.ts",
    "keywords": [
        "age",
        "file encryption",
        "cryptography"
    ],
    "dependencies": {
        "bech32-buffer": "^0.2.1",
        "libsodium-wrappers-sumo": "^0.7.11"
    },
    "scripts": {
        "test": "vitest --run",
        "examples:node": "cd tests/examples && npm install && node identity.js && node passphrase.js",
        "examples:bun": "cd tests/examples && bun install && bun run identity.js && bun run passphrase.js",
        "examples:esbuild": "cd tests/examples && npm install && esbuild --target=es6 --bundle --minify --outfile=age.js esbuild.js",
        "lint": "eslint .",
        "build": "tsc -p tsconfig.build.json",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@fast-check/vitest": "^0.0.6",
        "@types/libsodium-wrappers-sumo": "^0.7.5",
        "@types/node": "^20.4.8",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "eslint": "^8.45.0",
        "typescript": "^4.9.5",
        "vitest": "^0.34.1"
    },
    "author": "Filippo Valsorda <npm@filippo.io> (https://filippo.io)",
    "license": "BSD-3-Clause"
}
